        <phrase title="option_group_tapatalkOptions" version_id="60" version_string="2.4.2">
            <![CDATA[Tapatalk - Options]]></phrase>
        <phrase title="option_group_tapatalkOptions_description" version_id="60" version_string="2.4.2">
            <![CDATA[Additional options of your forums are available in Tapatalk Forum Owner Area]]></phrase>
        <phrase title="option_hideForums" version_id="60" version_string="2.4.2"><![CDATA[Hide Forums]]></phrase>
        <phrase title="option_hideForums_explain" version_id="60" version_string="2.4.2">
            <![CDATA[Hide forums and sub-forums you do not wish to be listed in Tapatalk]]></phrase>
        <phrase title="option_push_slug" version_id="15" version_string="1.5.2"><![CDATA[Push Slug]]></phrase>
        <phrase title="option_push_slug_explain" version_id="15" version_string="1.5.2">
            <![CDATA[Push Slug can stick push request when push failed several times in short time(cannot connect to push server).]]></phrase>
        <phrase title="option_readonlyForums" version_id="60" version_string="2.4.2">
            <![CDATA[Disable New Topic]]></phrase>
        <phrase title="option_readonlyForums_explain" version_id="60" version_string="2.4.2">
            <![CDATA[Prevent Tapatalk users from creating new topics in the selected sub-forums. This feature is useful for forums that require additional information or credentials that Tapatalk does not yet support.]]></phrase>
        <phrase title="option_Tag_Function_Open" version_id="60" version_string="2.4.2">
            <![CDATA[Open Tag Function]]></phrase>
        <phrase title="option_Tag_Function_Open_explain" version_id="60" version_string="2.4.2">
            <![CDATA[Enable support for Open Tagging (@). With this feature enabled, users can input "@username" to tag someone. Tagged users will receive alerts in their web browser and a push notification on their mobile device. To successfully tag someone, there must be one space before and after the "@username".]]></phrase>
        <phrase title="option_tapatalk_custom_replacement" version_id="60" version_string="2.4.2">
            <![CDATA[Thread Content Replacement (Advanced)]]></phrase>
        <phrase title="option_tapatalk_custom_replacement_explain" version_id="60" version_string="2.4.2">
            <![CDATA[Ability to match and replace thread content using PHP preg_replace function(http://www.php.net/manual/en/function.preg-replace.php). E.g. "'pattern', 'replacement'" . You can define more than one replace rule on each line.]]></phrase>
        <phrase title="option_tapatalk_push_notifications" version_id="60" version_string="2.4.2">
            <![CDATA[Push Notifications]]></phrase>
        <phrase title="option_tapatalk_push_notifications_explain" version_id="60" version_string="2.4.2"><![CDATA[<b>Basic Message</b> - Do not include post content and images preview in Push Notifications<br>
<b>Rich Message</b>  - Includes post content and images preview in Push Notifications]]></phrase>
        <phrase title="option_tapatalk_reg_type" version_id="60" version_string="2.4.2">
            <![CDATA[In-App Registration]]></phrase>
        <phrase title="option_tapatalk_reg_type_explain" version_id="60" version_string="2.4.2">
            <![CDATA[Verified Tapatalk users signed in from Facebook, Google or verified email address can register your forum natively in-app. Addition custom fields are also supported, although we strongly recommend to keep the custom fields to absolute minimal to make registration easier on mobile.]]></phrase>
        <phrase title="option_tapatalk_reg_ug" version_id="60" version_string="2.4.2">
            <![CDATA[User Group Assignment]]></phrase>
        <phrase title="option_tapatalk_reg_ug_explain" version_id="60" version_string="2.4.2">
            <![CDATA[By filling a usergroup ID, you can assign new users registered from Tapatalk to a specific secondary usergroup.]]></phrase>
        <phrase title="option_tapatalk_settings" version_id="11" version_string="2.4.2">
            <![CDATA[tapatalk_settings]]></phrase>
        <phrase title="option_tapatalk_settings_explain" version_id="11" version_string="2.4.2">
            <![CDATA[tapatalk_settings]]></phrase>
        <phrase title="option_tp_android_url" version_id="15" version_string="1.5.2">
            <![CDATA[Android Product URL]]></phrase>
        <phrase title="option_tp_android_url_explain" version_id="15" version_string="1.5.2">
            <![CDATA[Enter your BYO App URL from Google Play, to be used on Android device]]></phrase>
        <phrase title="option_tp_app_banner_msg" version_id="15" version_string="1.5.2">
            <![CDATA[BYO App Banner Message]]></phrase>
        <phrase title="option_tp_app_banner_msg_explain" version_id="15" version_string="1.5.2">
            <![CDATA[E.g. "Follow {your_forum_name} with {app_name} for [os_platform]". Do not change the [os_platform] tag as it is displayed dynamically based on user's device platform.]]></phrase>
        <phrase title="option_tp_app_ios_id" version_id="15" version_string="1.5.2"><![CDATA[BYO iOS App ID]]></phrase>
        <phrase title="option_tp_app_ios_id_explain" version_id="15" version_string="1.5.2">
            <![CDATA[Enter your product ID in Apple App Store, to be used on iPhone device]]></phrase>
        <phrase title="option_tp_directory" version_id="60" version_string="2.4.2">
            <![CDATA[Tapatalk Plugin Directory]]></phrase>
        <phrase title="option_tp_directory_explain" version_id="60" version_string="2.4.2">
            <![CDATA[This setting is for advanced users. If you installed the Tapatalk plug-in into a custom folder by manually changing the name of the default "mobiquo" folder, you must specify the custom folder name here. You will also need to specify the custom folder name in your <a href="https://tapatalk.com/v2/landing/index" target="_blank">Tapatalk forum owner account</a> as well.]]></phrase>
        <phrase title="option_tp_push_key" version_id="60" version_string="2.4.2"><![CDATA[Tapatalk Key]]></phrase>
        <phrase title="option_tp_push_key_explain" version_id="60" version_string="2.4.2">
            <![CDATA[This field is mandatory. The Tapatalk API key allows us to verify the origin of push notification requests from your server. Please input the key provided in your forum owner account. Inputting this key will insure that push notifications like Private Messages and Subscription activity can be delivered to your members' mobile devices.]]></phrase>
        <phrase title="option_full_banner" version_id="60" version_string="2.4.2"><![CDATA[Smart App Banner]]></phrase>
